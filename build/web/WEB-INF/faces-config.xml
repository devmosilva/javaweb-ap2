<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>paisController</managed-bean-name>
        <managed-bean-class>frb.edu.br.matheus.controller.PaisController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaListagem</from-outcome>
            <to-view-id>/paisListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidades</from-outcome>
            <to-view-id>/cityListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEndereco</from-outcome>
            <to-view-id>/addressListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paisListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/PaisIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/paisEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/paisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paisEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/paisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>cityController</managed-bean-name>
        <managed-bean-class>frb.edu.br.matheus.controller.CityController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/cityListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/cityIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/cityEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cityIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/cityListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>addressController</managed-bean-name>
        <managed-bean-class>frb.edu.br.matheus.controller.AddressController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/addressListagem.xhtml</from-view-id>
         <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/addressIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/AddressEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cityEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/cityListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/addressIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/addressListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/AddressEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/addressListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
